{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ]
        },
        "jsx": "react-jsx",
        "jsxImportSource": "react"
    },
    "exclude": [
        "_legacy_backup",
        "dist",
        "node_modules",
        "src/views/Home.tsx",
        "src/views/FAQ.tsx",
        "src/views/pages/FAQ.tsx",
        "src/components/SEO.tsx"
    ]
}